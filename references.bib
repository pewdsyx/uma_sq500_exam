	doi = {10.7287/peerj.preprints.1889v1},
	file = {Snapshot:C\:\\Users\\Yves Staudenmaier\\Zotero\\storage\\GV96U36C\\1889.html:text/html;Full Text PDF:C\:\\Users\\Yves Staudenmaier\\Zotero\\storage\\ZRN4JMRC\\Schermann et al. - 2016 - An empirical study on principles and practices of .pdf:application/pdf},
